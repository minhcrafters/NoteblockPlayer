{
  "schemaVersion": 1,
  "id": "noteblockplayer",
  "version": "${version}",
  "name": "NoteblockPlayer",
  "description": "A mod for building and playing noteblock songs in Minecraft",
  "authors": [
    "minhcrafters"
  ],
  "license": "MIT",
  "icon": "assets/noteblockplayer/icon.png",
  "environment": "*",
  "accessWidener" : "noteblockplayer.accesswidener",
  "entrypoints": {
    "main": [
      "me.minhcrafters.noteblockplayer.NoteblockPlayer"
    ],
    "modmenu": [
      "me.minhcrafters.noteblockplayer.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "noteblockplayer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.18",
    "fabric": "*",
    "minecraft": ">=1.20",
    "java": ">=17"
  },
  "suggests": {
    "flamingo": "*"
  }
}
